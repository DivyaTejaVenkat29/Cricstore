{
  "builds": [
    {
      "src": "cricstoreproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cricstoreproject/wsgi.py"  // Corrected path
    }
  ]
}
